<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="NewsWebSite" namespace="NewsWebSite.Models">
  <class name="Article" dynamic-update="true" >
    <cache usage="read-write"/>
    <id name="Id" type="int">
      <generator class="native" />
    </id>
    <property name="Title" />
    <property name="FullDescription" type="string" length="1000000"/>
    <property name="Image" />
    <property name="CreateDate" />
    <property name="LastUpdateDate" />
    <property name="UserId" />
    <property name="Tags" />
  </class>
</hibernate-mapping>